<% if atendimento %>
  <% if usuario_atual.secretaria? || usuario_atual.profissional ==  atendimento.profissional %>
    <div class="field">
      <label for="atendimento[modalidade_id]">Modalidade</label>
      <%== hx_select options: AtendimentoModalidade.all.map { |modalidade| [modalidade.modalidade, modalidade.id]}, target: "##{dom_id(atendimento)}", patch: atendimento_path(atendimento, authenticity_token: form_authenticity_token.to_s, no_redirect: true), name: "atendimento[modalidade_id]", swap: "outerHTML", value: atendimento.modalidade_id %>
    </div>
  <% else %>
    <p><%= atendimento.modalidade.upcase %></p>
  <% end %>
<% end %>
