<% if !atendimento %>
<% else %>
  <% if atendimento.instrumentos.empty? %>
    <center>-</center>
  <% else %>
    <ul>
      <% atendimento.instrumentos.each do |instrumento| %>
        <li>
          <%= instrumento.nome %>
        </li>
      <% end %>
    </ul>
  <% end %>
  <br>
  <%#== hx_select options: Instrumento.order(nome: :asc).map { |instrumento| [instrumento.nome, instrumento.id]}, post: instrumento_relatos_path(ajax: true, authenticity_token: form_authenticity_token.to_s) %>
<% end %>
