<div class="grid-3" id="profissional-acompanhamentos-resumo">
<% profissional.acompanhamentos.em_andamento.order(data_inicio: :desc).first(5).each do |a| %>
  <%= render 'acompanhamentos/card', acompanhamento: a %>
<% end %>
<%= link_to acompanhamentos_profissional_path(profissional), class: 'card', style: 'display:grid;align-content:center;' do %>
<div>
  <h2 style="text-align:center;">Ver todos</h2>
</div>
<% end %>
</div>
