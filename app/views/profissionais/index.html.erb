<p id="notice"><%= notice %></p>

<h1>Profissionais</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>Feminino</th>
      <th>Cpf</th>
      <th>Civil estado</th>
      <th>Instrucao grau</th>
      <th>Data nascimento</th>
      <th>Funcao</th>
      <th>Municipio</th>
      <th>Endereco cep</th>
      <th>Endereco logradouro</th>
      <th>Endereco numero</th>
      <th>Endereco complemento</th>
      <th>Fone cod pais</th>
      <th>Fone cod area</th>
      <th>Fone num</th>
      <th>Email</th>
      <th>Bio</th>
      <th>Salario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profissionais.each do |profissionai| %>
      <tr>
        <td><%= profissionai.nome %></td>
        <td><%= profissionai.sobrenome %></td>
        <td><%= profissionai.feminino %></td>
        <td><%= profissionai.cpf %></td>
        <td><%= profissionai.civil_estado %></td>
        <td><%= profissionai.instrucao_grau %></td>
        <td><%= profissionai.data_nascimento %></td>
        <td><%= profissionai.funcao %></td>
        <td><%= profissionai.municipio %></td>
        <td><%= profissionai.endereco_cep %></td>
        <td><%= profissionai.endereco_logradouro %></td>
        <td><%= profissionai.endereco_numero %></td>
        <td><%= profissionai.endereco_complemento %></td>
        <td><%= profissionai.fone_cod_pais %></td>
        <td><%= profissionai.fone_cod_area %></td>
        <td><%= profissionai.fone_num %></td>
        <td><%= profissionai.email %></td>
        <td><%= profissionai.bio %></td>
        <td><%= profissionai.salario %></td>
        <td><%= link_to 'Show', profissionai %></td>
        <td><%= link_to 'Edit', edit_profissionai_path(profissionai) %></td>
        <td><%= link_to 'Destroy', profissionai, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profissionai', new_profissionai_path %>
