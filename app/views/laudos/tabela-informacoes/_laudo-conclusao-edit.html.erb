<form id="laudo-conclusao-edit"
      hx-target="#informacoes"
      hx-patch="<%= informacoes_update_laudo_path(laudo) %>"
      hx-target="#identificacao">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <textarea style="width:90%;height:10em;" name="laudo[conclusao]" value="<%= laudo.conclusao %>" placeholder="Descreva a conclusao detalhadamente"><%= laudo.conclusao %></textarea>
  <br>
  <button>Ok</button>
  <button hx-get="<%= conclusao_laudo_path(laudo) %>" hx-target="#laudo-conclusao-edit" hx-swap="outerHTML">Cancelar</button>
</form>

