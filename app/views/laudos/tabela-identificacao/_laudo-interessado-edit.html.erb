<form id="laudo-interessado-edit"
      hx-patch="<%= identificacao_update_laudo_path(laudo) %>"
      hx-target="#identificacao">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="text" name="laudo[interessado]" value="<%= laudo.interessado %>" placeholder="Deixe em branco se o interessado for o paciente">
  <button>Ok</button>
  <button hx-get="<%= interessado_laudo_path(laudo) %>" hx-target="#laudo-interessado-edit" hx-swap="outerHTML">Cancelar</button>
</form>
