<div id="<%= dom_id biblioteca_obra %>">
  <p>
    <strong>Titulo:</strong>
    <%= biblioteca_obra.titulo %>
  </p>

  <p>
    <strong>Subtitulo:</strong>
    <%= biblioteca_obra.subtitulo %>
  </p>

  <p>
    <strong>Tipo de obra</strong>
    <%= biblioteca_obra.biblioteca_obra_tipo&.tipo&.upcase %>
  </p>

  <p>
    <strong>Editora</strong>
    <%= biblioteca_obra.biblioteca_editora&.nome&.upcase %>
  </p>

  <p>
    <strong>Ordem:</strong>
    <%= biblioteca_obra.ordem %>
  </p>

  <p>
    <strong>Data publicacao:</strong>
    <%= biblioteca_obra.data_publicacao %>
  </p>

  <p>
    <strong>Isbn:</strong>
    <%= biblioteca_obra.isbn %>
  </p>

  <p>
    <strong>Caminho:</strong>
    <%= biblioteca_obra.caminho %>
  </p>

  <p>
    <strong>Obra virtual:</strong>
    <%= biblioteca_obra.obra_virtual %>
  </p>

  <p>
    <strong>Num paginas:</strong>
    <%= biblioteca_obra.num_paginas %>
  </p>

  <p>
    <strong>Resumo:</strong>
    <%= biblioteca_obra.resumo %>
  </p>

  <p>
    <strong>Edicao:</strong>
    <%= biblioteca_obra.edicao %>
  </p>

  <p>
    <strong>Volume:</strong>
    <%= biblioteca_obra.volume %>
  </p>

  <p>
    <strong>Biblioteca periodico:</strong>
    <%= biblioteca_obra.biblioteca_periodico&.titulo&.upcase %>
  </p>
</div>
