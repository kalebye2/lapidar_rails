<% if @usuario %>
  <table class="tabela-infantojuvenil-anamnese" style="width:90%;margin:auto;" id='tabela-anamnese'>
    <%= render "tabela-dados-principais", infantojuvenil_anamnese: @infantojuvenil_anamnese %>
    <tbody>
      <%= render "tabela-gestacao", gestacao: @infantojuvenil_anamnese.gestacao %>
      <%= render "tabela-alimentacao", alimentacao: @infantojuvenil_anamnese.alimentacao %>
      <%= render "tabela-psicomotricidade", psicomotricidade: @infantojuvenil_anamnese.psicomotricidade %>
      <%= render "tabela-sono", sono: @infantojuvenil_anamnese.sono %>
      <%= render "tabela-socioafetividade", socioafetividade: @infantojuvenil_anamnese.socioafetividade %>
      <%= render "tabela-comunicacao", comunicacao: @infantojuvenil_anamnese.comunicacao %>
      <%= render "tabela-manipulacao", manipulacao: @infantojuvenil_anamnese.manipulacao %>
      <%= render "tabela-saude_historico", saude_historico: @infantojuvenil_anamnese.saude_historico %>
      <%= render "tabela-escola_historico", escola_historico: @infantojuvenil_anamnese.escola_historico %>
      <%= render "tabela-sexualidade", sexualidade: @infantojuvenil_anamnese.sexualidade %>
      <%= render "tabela-familia_historico", familia_historico: @infantojuvenil_anamnese.familia_historico %>
    </tbody>
  </table>
<% else %>
  <div class="documento">
    <%= render "documento-dados-principais", infantojuvenil_anamnese: @infantojuvenil_anamnese %>
    <%= render "documento-gestacao", gestacao: @infantojuvenil_anamnese.gestacao %>
    <%= render "documento-alimentacao", alimentacao: @infantojuvenil_anamnese.alimentacao %>
    <%= render "documento-psicomotricidade", psicomotricidade: @infantojuvenil_anamnese.psicomotricidade %>
    <%= render "documento-sono", sono: @infantojuvenil_anamnese.sono %>
    <%= render "documento-socioafetividade", socioafetividade: @infantojuvenil_anamnese.socioafetividade %>
    <%= render "documento-comunicacao", comunicacao: @infantojuvenil_anamnese.comunicacao %>
    <%= render "documento-manipulacao", manipulacao: @infantojuvenil_anamnese.manipulacao %>
    <%= render "documento-saude_historico", saude_historico: @infantojuvenil_anamnese.saude_historico %>
    <%= render "documento-escola_historico", escola_historico: @infantojuvenil_anamnese.escola_historico %>
    <%= render "documento-sexualidade", sexualidade: @infantojuvenil_anamnese.sexualidade %>
    <%= render "documento-familia_historico", familia_historico: @infantojuvenil_anamnese.familia_historico %>
  </div>
<% end %>
