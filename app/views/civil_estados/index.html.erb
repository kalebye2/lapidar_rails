<p id="notice"><%= notice %></p>

<h1>Civil Estados</h1>

<table>
  <thead>
    <tr>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @civil_estados.each do |civil_estado| %>
      <tr>
        <td><%= civil_estado.estado %></td>
        <td><%= link_to 'Show', civil_estado %></td>
        <td><%= link_to 'Edit', edit_civil_estado_path(civil_estado) %></td>
        <td><%= link_to 'Destroy', civil_estado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Civil Estado', new_civil_estado_path %>
